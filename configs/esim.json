{
    "max_length": 100,
    "char_limit": 10,
    "scope":"ESIM",
    "num_classes":160000,

    "with_char": false,
    "char_emb_dim": 20,
    "char_lstm_dim": 40,

    "batch_size": 1024,
    "max_epochs": 1000,
    "dropout_rate": 0.2,
    "learning_rate": 1e-3,
    "optimizer": "adam",
    "lambda_l2": 0.0,
    "grad_clipper": 10.0,

    "context_layer_num": 1,
    "context_lstm_dim": 100,

    "slstm_layer_num": 5,

    "rnn":"lstm",
    "random_initialize":false,
    "step":5,
    "slstm_hidden_size":300,
    "weight_decay":1e-5,

    "highway_layer_num": 1,
    "with_highway": true,
    "with_match_highway": true,
    "with_aggregation_highway": true,

    "use_cudnn": false,
    "with_center_loss":true,

    "with_moving_average": 0.999,

    "loss":"focal_loss_multi_v1",

    "alpha":0.5,
    "gamma":2.0,
    "name":"focal_loss_multi_v1",
    "center_gamma":0.01,

    "with_word_drop":true,
    "word_drop_rate":0.7,

    "scale":30,
    "margin":0.35
}
